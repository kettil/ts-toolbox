{
  "name": "@kettil/eslint-config",
  "version": "0.2.0",
  "description": "The library is a collection of very strict ESLint rules",
  "main": "node.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "eslint --config .eslintrc.json --fix .",
    "lint:ci": "eslint --config .eslintrc.json --format codeframe .",
    "prettier": "prettier --config './.prettierrc' --write '.prettierrc' './**/*.{ts,tsx,js,jsx,json,md,scss,yml,html}'",
    "prettier:ci": "prettier --config './.prettierrc' --list-different '.prettierrc' './**/*.{json,md,scss,yml,html}'"
  },
  "keywords": [
    "ecmascript",
    "es6",
    "es2015",
    "es2016",
    "es2017",
    "es2018",
    "es2019",
    "es2020",
    "eslint",
    "eslint-config",
    "jest",
    "react",
    "typescript"
  ],
  "author": "Kjell Die√üel <development@kettil.de>",
  "license": "MIT",
  "engines": {
    "node": ">= 10"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kettil/eslint-config"
  },
  "bugs": {
    "url": "https://github.com/kettil/eslint-config/issues/new"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^2.31.0",
    "@typescript-eslint/parser": "^2.31.0",
    "eslint-plugin-formatjs": "^2.2.3",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.9.0",
    "eslint-plugin-jest-dom": "^2.1.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^4.0.0",
    "eslint-plugin-testing-library": "^3.1.0",
    "eslint-plugin-unicorn": "^19.0.1"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "eslint": "^6.8.0",
    "prettier": "^2.0.5",
    "semantic-release": "^17.0.7",
    "typescript": "^3.8.3"
  }
}
